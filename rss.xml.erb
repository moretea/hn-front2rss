<rss xmlns:atom="http://www.w3.org/2005/Atom" version="2.0">
  <channel>
    <title>Hacker News Frontpage</title>
    <link>http://news.ycombinator.com</link>
    <language>en</language>
    <description>Hacker News Frontpage</description>
    <atom:link rel="self" href="http://localhost:83/hn-rss.xml" type="application/rss+xml"/>
    <% items.each do |item| %>
      <item>
        <title><%= item[:title] %></title>
        <guid><%= item[:id] %></guid>
        <link><%= item[:commentLink] %></link>
      </item>
    <% end %>
  </channel>
</rss>
